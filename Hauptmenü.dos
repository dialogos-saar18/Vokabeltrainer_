			<name>Ohne Titel</name>
			<value>#ABNF 1.0 ISO-8859-1
language Deutsch;
root $Ausw;
public $ausw = $Abfragen {$ = &quot;abfragen&quot;} | $Wörterbuch {$ = &quot;wörterbuch&quot;} | $Lektion {$ = lektion};
$Abfragen = ( abfragen ) [ lernen ];
$Wörterbuch = ( wörterbuch) ;
$Lektion = { Lektion };</value>
		</grammar>
		<grammar id="73de46e7-e92b-4e65-9464-3d915051c2f0">
			<name>JaNein</name>
			<value>#ABNF 1.0 ISO-8859-1
language Deutsch;
root $janein;
public $janein = $ja {$ = &quot;ja&quot;} | $nein {$ = &quot;nein&quot;};
$ja = ( ja | jawohl ) [ bitte | gerne ] | richtig | korrekt;
$nein = ( nein | ne | falsch );</value>
		</grammar>
		<grammar id="6f88a9c0-76ae-4217-a83d-8d07943f9607">
			<name>Grammatik1</name>
			<value>#ABNF 1.0 ISO-8859-1
language Deutsch;
root $gram1;
public $gram1 = $ja {$ = &quot;ja&quot;} | $nein {$ = &quot;nein&quot;} | $abfrage {$ = &quot;abfrage&quot;} | $lektion {$ = &quot;lektion&quot;} | $wörterbuch {$ = &quot;wörterbuch&quot;} | $hä {$ = &quot;hä&quot;};
$ja = ( ja | jawohl ) [ bitte | gerne ] | richtig | korrekt;
$nein = ( nein | ne | falsch | nichts);
$abfrage = ((Abfrage | [Vokabeln | Wörter] abfragen) [bitte] | Frag mich [bitte | mal] ab) | (Vokabel Test) [bitte];
$lektion = ((Vokabel (Listen | Liste)  ) [verwalten] [bitte]) | (Ich (möchte | will) meine (Vokabel Listen) [bitte] [verwalten] [bitte]) | (Ich (möchte | will) [meine] Vokabel Listen [verwalten]);
$hä = (Was (kann | kannst) (ich | du) [nochmal | denn] (tun | machen)) | (Was sind meine (Möglichkeiten | Optionen)) | Hä;
$wörterbuch = [Ich hätte gerne (ein | das)] Wörterbuch [bitte] | [Ich (muss | will | möchte) etwas] nachschlagen | (Ich brauche ein Wort);
</value>
		</grammar>
